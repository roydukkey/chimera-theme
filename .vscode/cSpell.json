{
	"version": "0.2",
	"ignorePaths": [
		"dist",
		"node_modules/**/{.*/**/,.*/**/.,,.}*",
		".git/**",
		"**/.DS_Store",
		"*.lock.json",
		"*-lock.json",
		"*.vsix"
	],
	"dictionaries": [
		"fullstack",
		"lua",
		"rust",
		"typescript"
	],
	// Before adding a word, see if there is already a dictionary in which it is contained.
	// http://github.com/streetsidesoftware/cspell-dicts
	"words": [
		"alignas",
		"dbaeumer",
		"desaturate",
		"editorconfig",
		"enummember",
		"gitlens",
		"noexcept",
		"preprocessors",
		"roydukkey",
		"streetsidesoftware",
		"stroustrup",
		"tsdoc",
		"uintptr",
		"unsets",
		"valpha",
		"vendored",
		"vsix",
		"wordlike"
	]
}
